{
  "name": "s4apitestproj",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.15.7",
    "@cap-js/hana": "^2",
    "@sap-cloud-sdk/connectivity": "^4.1.2",
    "@sap-cloud-sdk/http-client": "^4.1.2",
    "@sap-cloud-sdk/resilience": "^4.1.2",
    "@sap-cloud-sdk/util": "^4.1.2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "cds-swagger-ui-express": "^0.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": "^9"
  },
  "scripts": {
    "start": "cds-serve"
  },
  "private": true,
  "cds": {
    "server": {
      "index": true
    },
    "requires": {
      "[production]": {
        "auth": "xsuaa"
      },
      "ZRK_API_AGENCY_V4": {
        "kind": "odata-v2",
        "model": "srv/external/ZRK_API_AGENCY_V4",
        "requestTimeout": 60000,
        "[development]": {
          "credentials": {
            "destination": "RJTechSphere",
            "path": "/sap/opu/odata4/sap/zrk_api_agency_v4/srvd_a2x/dmo/ui_agency/0001"
          }
        },
        "[production]": {
          "credentials": {
            "destination": "RJTechSphere",
            "path": "/sap/opu/odata4/sap/zrk_api_agency_v4/srvd_a2x/dmo/ui_agency/0001"
          }
        },
        "[hybrid]": {
          "credentials": {
            "destination": "RJTechSphere",
            "path": "/sap/opu/odata4/sap/zrk_api_agency_v4/srvd_a2x/dmo/ui_agency/0001"
          }
        }
      }
    }
  }
}